@{
    ViewBag.Title = "Contacts";
}



<div class="row">
    <div class="col-offset-1 col s5">
        <h2>@ViewBag.Title</h2>
    </div>
    <div class="col s6">
        <div class="card">
            <div class="card-image">
                <img src="~/Img/portrait-photo_vasil.png">
                <span class="card-title">Vasil Kamburov</span>
            </div>
            <div class="card-content">
                <p>
                    Dedicated Developer!Loves MVC and Angular!
                </p>
            </div>
            <div class="card-action">
                <a href="https://www.facebook.com/vasil.kamburov"><img src="~/Img/profile__icons-facebook.png" alt="facebook" class="profile__icons"></a>
                <a href="https://github.com/vasil87 "><img src="~/Img/profile__icons-github.png" alt="github" class="profile__icons "></a>
            </div>
        </div>
    </div>
</div>
